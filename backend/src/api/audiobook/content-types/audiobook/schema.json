{
  "kind": "collectionType",
  "collectionName": "audiobooks",
  "info": {
    "singularName": "audiobook",
    "pluralName": "audiobooks",
    "displayName": "Audiobook",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "avgScore": {
      "type": "decimal",
      "max": 5,
      "min": 0
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "audiobooks"
    },
    "loanedBy": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "audiobooksLoaned"
    },
    "datePublished": {
      "type": "date"
    },
    "isbn": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "richtext"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "audiobooksWritten"
    },
    "length": {
      "type": "integer"
    },
    "narratedBy": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "audiobooksNarrated"
    }
  }
}
